<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<add key="Enabled" value="True" />
		<!-- Configuration -->
		<add key="Namespace" value="Embeddings" />
		<add key="DefaultColumnSummary" value="{0}" />
		<add key="DefaultTableSummary" value="{0}" />
		<add key="EnableInernalFiles" value="True" />
		<add key="OutputPath" value=".\Embeddings" />
		<add key="ModelClassName" value="EmbeddingsContext" />
		<!-- Item -->
		<add key="GenerateCloneAndCopyMethods" value="True" />
		<add key="GenerateOneToManyItems" value="True" />
		<add key="GenerateOneToManyLists" value="True" />
		<!-- Context -->
		<add key="GenerateContext" value="True" />
		<add key="ContextNamespace" value="Embeddings" />
		<!-- DataAccess -->
		<add key="GenerateDataAccessClasses" value="False" />
		<!-- Controllers -->
		<add key="GenerateControllers" value="False" />
		<add key="ControllerNamespace" value="Embeddings.Controllers" />
		<add key="ControllerUseActionResult" value="True" />
		<add key="ControllerBaseClassList" value="ControllerBase" />
		<!-- DTO -->
		<add key="GenerateDTO" value="True" />
		<!-- Pluralize -->
		<add key="PluralizeTableName" value="True" />
		<add key="SingularizeItemName" value="True" />
		<add key="PluralizeForeignKeyLists" value="True" />
		<add key="SingularizeForeignKeyItems" value="True" />
		<!-- C# Features -->
		<add key="EnableNullableReferenceTypes" value="False" />
		<add key="EnableEntityFramework6" value="True" />
		<add key="EnableEntityFrameworkCore" value="True" />
		<!-- Valid solution would be to use DateTimeOffset type on SQL and C#. -->
		<add key="UseDateTimeKindUtcAttribute" value="False" />
		<add key="UseDateTimeKindUtcProperty" value="False" />
		<add key="StoreEnumDataAsStrings" value="True" />
		<!-- Lines will be trimmed and joined with '|' into a regular expression. -->
		<add key="ExcludeFilter" value="\b__" />
		<add key="IncludeFilter" value="" />
		<add key="OverrideColumnInfo" value="
			Id,CustomType,IsEnum
		" />
	</appSettings>
	<connectionStrings>
		<add
			name="ModelConnection"
			connectionString="Integrated Security=True;Persist Security Info=False;Initial Catalog=Embeddings;Data Source=localhost"
			providerName="System.Data.EntityClient"/>
	</connectionStrings>
</configuration>


